{
  "name": "react-ant-design",
  "version": "0.1.0",
  "private": true,
  "homepage": ".",
  "scripts": {
    "commit": "git-cz",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:code": "eslint --fix **/*.{js,jsx,tsx}",
    "lint:style": "stylelint --fix src/**/*.{css,less,scss,sass}",
    "start": "node scripts/start.js",
    "build:dev": "cross-env REACT_ENV=development node scripts/build.js",
    "build:pro": "cross-env REACT_ENV=production node scripts/build.js",
    "test": "node scripts/test.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{css,less,scss,sass}": [
      "stylelint --fix src/**/*.{css,less,scss,sass}",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@ant-design/pro-layout": "^4.5.14",
    "ant-design-pro": "^2.3.2",
    "antd": "^3.22.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "enquire-js": "^0.2.1",
    "gg-editor": "^2.0.4",
    "good-storage": "^1.1.0",
    "lodash": "^4.17.15",
    "lodash-decorators": "^6.0.1",
    "moment": "^2.24.0",
    "nzh": "^1.0.4",
    "path-to-regexp": "^3.0.0",
    "rc-drawer": "^2.0.6",
    "react": "^16.9.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/plugin-transform-react-jsx-self": "^7.2.0",
    "@babel/plugin-transform-react-jsx-source": "^7.5.0",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@svgr/webpack": "4.3.2",
    "@types/classnames": "^2.2.9",
    "@types/jest": "24.0.17",
    "@types/lodash": "^4.14.137",
    "@types/node": "12.7.2",
    "@types/qs": "^6.5.3",
    "@types/react": "16.9.2",
    "@types/react-document-title": "^2.0.3",
    "@types/react-dom": "16.8.5",
    "@types/react-loadable": "^5.5.1",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.4",
    "@types/redux-logger": "^3.0.7",
    "@types/webpack-env": "^1.14.0",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "^24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "^1.12.0",
    "babel-plugin-named-asset-import": "^0.3.3",
    "babel-preset-react-app": "^9.0.1",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "commitizen": "^3.1.2",
    "compression-webpack-plugin": "^3.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "dotenv": "6.2.0",
    "dotenv-expand": "4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-loader": "2.2.1",
    "eslint-plugin-flowtype": "2.50.3",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "3.0.1",
    "fs-extra": "7.0.1",
    "html-webpack-plugin": "4.0.0-beta.8",
    "husky": "^2.7.0",
    "identity-obj-proxy": "3.0.0",
    "is-wsl": "^1.1.0",
    "jest": "24.9.0",
    "jest-environment-jsdom-fourteen": "0.1.0",
    "jest-resolve": "24.9.0",
    "jest-watch-typeahead": "0.3.1",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "lint-staged": "^8.2.1",
    "mini-css-extract-plugin": "0.8.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.5.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "7.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "progress-bar-webpack-plugin": "^1.12.1",
    "react-app-polyfill": "^1.0.2",
    "react-dev-utils": "^9.0.3",
    "redux-logger": "^3.0.6",
    "resolve": "1.12.0",
    "sass-loader": "7.2.0",
    "semver": "6.3.0",
    "style-loader": "0.23.1",
    "stylelint": "^10.1.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-order": "^3.0.1",
    "terser-webpack-plugin": "1.4.1",
    "ts-pnp": "1.1.2",
    "typescript": "3.5.3",
    "url-loader": "1.1.2",
    "webpack": "4.39.2",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-dev-server": "3.8.0",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "4.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        },
        "antd"
      ],
      [
        "import",
        {
          "libraryName": "ant-design-pro",
          "libraryDirectory": "lib",
          "style": true,
          "camel2DashComponentName": false
        },
        "ant-design-pro"
      ]
    ],
    "presets": [
      "react-app"
    ]
  }
}
